<Workflow Name="SyncSegment">
   <Connectors>
      <Connector ID="WebBFS" Name="WebBFS">
         <SearchObjects>
            <SearchObject ID="FetchPlantItems"  Name="FetchPlantItems">
               <ListOf_DataTables>
                  <DataTable ID="Anl" Name="Anl">
                     <Columns>
                        <ColumnName>AnlKnz</ColumnName>
                        <ColumnName>KlaKnz</ColumnName>
                        <ColumnName>APMKla</ColumnName>
                        <ColumnName>AnlSta</ColumnName>
                        <ColumnName>AnlBez</ColumnName>
                        <ColumnName>SyncToAPM</ColumnName>
                     </Columns>
                  </DataTable>
                  <DataTable ID="Asr" Name="Asr">
                     <Columns>
                        <ColumnName>AsrNum</ColumnName>
                     </Columns>
                  </DataTable>
               </ListOf_DataTables>
            </SearchObject>
         </SearchObjects>
      </Connector>
   </Connectors>
   <Transformations>
      <Transformation ID="SearchAnlToAnlMemory" Name="SearchAnlToAnlMemory" sourceObjectName = "Anl" destintationObjectName = "AnlMemory">
	     <ProducerID>Anl</ProducerID>
		 <ConsumerID>AnlMemory</ConsumerID>
         <Mapping>
            <SourceColName>AnlKnz</SourceColName>
            <DestinationColName>anlknz</DestinationColName>
         </Mapping>
         <Mapping>
            <SourceColName>KlaKnz</SourceColName>
            <DestinationColName>klaknz</DestinationColName>
         </Mapping>
         <Mapping>
            <SourceColName>APMKla</SourceColName>
            <DestinationColName>apmkla</DestinationColName>
         </Mapping>
         <Mapping>
            <SourceColName>AnlSta</SourceColName>
            <DestinationColName>anlsta</DestinationColName>
         </Mapping>
         <Mapping>
            <SourceColName>AnlBez</SourceColName>
            <DestinationColName>anlbez</DestinationColName>
         </Mapping>
         <Mapping>
            <SourceColName>SyncToAPM</SourceColName>
            <DestinationColName>sync_to_apm</DestinationColName>
         </Mapping>
      </Transformation>
      <Transformation Name="SearchAsrToAsrMemory" sourceObjectName = "Asr" destintationObjectName = "AsrMemory">
         <Mapping>
            <SourceColName>AsrNum</SourceColName>
            <DestinationColName>asrnum</DestinationColName>
         </Mapping>
      </Transformation>
   </Transformations>
   <Memories>
      <Memory ID="AnlMemory" Name="AnlMemory">
         <Columns>
            <Column>anlknz</Column>
            <Column>klaknz</Column>
            <Column>apmkla</Column>
            <Column>anlsta</Column>
            <Column>anlbez</Column>
            <Column>sync_to_apm</Column>
         </Columns>
      </Memory>
      <Memory ID="AsrMemory" Name="AsrMemory">
         <Columns>
            <Column>asrnum</Column>
         </Columns>
      </Memory>
   </Memories>
   <Variables>
      <Variable>
         <Name>kks</Name>
         <Value>70ADA</Value>
      </Variable>
	  <Variable>
		 <Name>webbfs_class</Name>
         <Value>Valve</Value>
      </Variable>
	  <Variable>
		 <Name>APM_class</Name>
         <Value>VALVE</Value>
      </Variable>
	  <Variable>
		 <Name>status</Name>
         <Value>1</Value>
      </Variable>
	  <Variable>
		 <Name>designation</Name>
         <Value>test</Value>
      </Variable>
   </Variables>
   <Assignments>
	<SetValue ID="FromMemory" Name="FromMemory">
		<Mapping>
            <VariableName>AnlKnz</VariableName>
            <SourceColumnName>anlknz</SourceColumnName>
        </Mapping>
	</SetValue>
   </Assignments>
</Workflow>